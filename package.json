{
  "name": "mifosx-web-app",
  "version": "0.0.0",
  "description": "MifosX Web App is the default web application built on top of the Fineract platform for the Mifos user community leveraging the popular Angular framework.",
  "keywords": [
    "mifos",
    "openmf",
    "fineract",
    "angular"
  ],
  "homepage": "https://github.com/openMF/web-app",
  "license": "MPL-2.0",
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --configuration production --output-hashing=none",
    "build:prod": "node --max-old-space-size=16384 ./node_modules/@angular/cli/bin/ng build --configuration production --output-hashing=none --base-href=/web-app/",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/animations": "13.1.3",
    "@angular/cdk": "13.1.3",
    "@angular/common": "13.1.3",
    "@angular/compiler": "13.1.3",
    "@angular/core": "13.1.3",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "13.1.3",
    "@angular/localize": "13.1.3",
    "@angular/material": "13.1.3",
    "@angular/platform-browser": "13.1.3",
    "@angular/platform-browser-dynamic": "13.1.3",
    "@angular/router": "13.1.3",
    "@angular/service-worker": "13.1.3",
    "@ckeditor/ckeditor5-angular": "1.2.3",
    "@ckeditor/ckeditor5-build-classic": "21.0.0",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "1.2.18",
    "@fortawesome/free-solid-svg-icons": "5.8.2",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "^6.0.0",
    "chart.js": "3.0.0-alpha",
    "core-js": "2.5.0",
    "lodash": "4.17.21",
    "rxjs": "6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.4",
    "@angular/cli": "13.1.4",
    "@angular/compiler-cli": "13.1.3",
    "@angular/language-service": "13.1.3",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@ngx-rocket/scripts": "3.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.132",
    "@types/node": "12.11.1",
    "codelyzer": "^6.0.0",
    "hads": "1.7.3",
    "htmlhint": "0.11.0",
    "https-proxy-agent": "2.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.12",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-junit-reporter": "1.2.0",
    "protractor": "~7.0.0",
    "stylelint": "10.0.1",
    "stylelint-config-recommended-scss": "3.3.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.6.1",
    "ts-node": "8.2.0",
    "tslint": "~6.1.0",
    "typescript": "4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openMF/web-app.git"
  },
  "bugs": {
    "url": "https://github.com/openMF/web-app/issues"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  }
}
